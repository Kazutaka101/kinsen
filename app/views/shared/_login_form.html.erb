
<%= @page_title = "Login" %>

<%= form_tag :sessions, id:"login_form" do %>
    <div>
        <label>User Name</label>
        <input type = "text" name = "name">
    </div>
    <div>
        <label>Password</label>
        <input type = "password", name = "password">
    </div>
    <div>
        <input type = "submit" value = "Login">
    </div>
<% end %>